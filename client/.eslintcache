AR\n                        </Button>\n                        \n                        }\n                        {props.tipoFormulario === \"alterarAnuncio\" &&\n                            <Button\n                                onClick={() => { AtualizarDadosBD() }}\n                                variant=\"contained\"\n                                color=\"default\"\n                                className={classes.button}\n                                startIcon={<CloudUploadIcon />}\n                            >\n                                ALTERAR ANÚNCIO\n                        </Button>\n                        }\n                        {props.tipoFormulario === \"deletarAnuncio\" &&\n                            <Button\n                                variant=\"contained\"\n                                color=\"secondary\"\n                                className={classes.button}\n                                startIcon={<DeleteIcon />}\n                            >\n                                REMOVER ANÚNCIO\n                            </Button>\n                        }\n                    </div>\n\n                </form>\n            </div >\n\n        </>\n    )\n}\n","/home/marco/Documents/project_react/template-carros/client/src/components/painelAdministrativo/publicacao/opcionais.jsx",[],["45","46"],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":125,"column":25,"nodeType":"53","endLine":125,"endColumn":124},{"ruleId":"54","severity":1,"message":"55","line":174,"column":60,"nodeType":"56","messageId":"57","endLine":174,"endColumn":62},{"ruleId":"58","severity":1,"message":"59","line":207,"column":13,"nodeType":"60","messageId":"61","endLine":210,"endColumn":14},{"ruleId":"62","severity":1,"message":"63","line":287,"column":7,"nodeType":"64","endLine":287,"endColumn":40,"suggestions":"65"},{"ruleId":"66","severity":1,"message":"67","line":637,"colum